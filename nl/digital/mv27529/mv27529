<script type="text/javascript" src="/wp-content/themes/betheme/js/scripts.min.js?ver=27.2.12" id="mfn-scripts-js"></script>
<script type="text/javascript" id="mfn-scripts-js-after">
/* <![CDATA[ */
// 10/11/2023 Note to whomever will adapt this. 
// If you're going to make changes here, could you check
// in with Vince Smolucha or Magalie Chetrit?
// 
// 11/20/2023
// Added failsafe logic to prevent JS errors on pages that
// do not have these elements.

// add any code in the DOMContentLoaded event
document.addEventListener("DOMContentLoaded", (event) => {

		const menuBtn 			= document.querySelector('.menu-btn');
		const mainNav 			= document.querySelector('.hmd-main-nav');
		const menuListItems	= document.querySelectorAll(".menu li a");
		
    // Set up a counter to keep track of which item is selected
		let currentItem = 0;
		function toggleMenu() {

        mainNav.classList.toggle('hmd-menu-open');
        document.querySelector('body').classList.toggle('hmd-mobile-menu-open');
        // Initialize first li as the selected (focused) one when mainNav is open
        menuListItems[currentItem].focus();
        menuBtn.setAttribute('aria-expanded', 'true');
        if (!document.querySelector('.hmd-main-nav').classList.contains('hmd-menu-open')) {
            menuBtn.setAttribute('aria-expanded', 'false');
        }
    }
 
	if(menuBtn){
	 // on event helps open the mobile nav
		menuBtn.addEventListener("mouseup", (event) => {
			toggleMenu();
		});
		
		menuListItems.forEach((element) => {
			element.addEventListener("mouseup", (event) => {
				toggleMenu();
				this.focus();
			});
		});
	}
	
	if(menuBtn && mainNav && (menuListItems.length > 0)){
		document.addEventListener("keydown", (event) => {
			// Check for up/down ESC/Enter key presses
			switch(event.code){
				case "ArrowUp": // Up arrow    
					// Remove focus from the previous element
					menuListItems[currentItem].blur();
					// Decrease the counter 
					currentItem = currentItem > 0 ? --currentItem : 0;
					// focus on the previous item
					menuListItems[currentItem].focus();
					break;
				case "ArrowDown": // Down arrow
					// Remove focus from the previous element
					menuListItems[currentItem].blur();
					// Increase counter 
					currentItem = currentItem < menuListItems.length-1 ? ++currentItem : menuListItems.length-1; 
					// Focus on the next element
					menuListItems[currentItem].focus();    
					break;
				case "Escape":
					// Remove focus from the selected element
					menuListItems[currentItem].blur();
					// set focus on the menubtn
					menuBtn.focus();
					// close menu if the main menu is open.
					mainNav.classList.remove('hmd-menu-open');
					// set aria expanded to false
					document.querySelector('.menu-btn').setAttribute("aria-expanded", "false");
					break;
				case "Enter":
					menuBtn.setAttribute('aria-expanded', 'true');
					mainNav.classList.toggle('hmd-menu-open');

					// Initialize first li as the selected (focused) one when mainNav is open
					if (!mainNav.classList.contains('hmd-menu-open')) {			
					 menuBtn.setAttribute('aria-expanded', 'false');
					}
					break;
				default:
					break;
				}
		});
	}

});
/* ]]> */
</script>
